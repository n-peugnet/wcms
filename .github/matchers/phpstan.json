{
    "problemMatcher": [
        {
            "owner": "phpstan",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):(.+)$",
                    "file": 1,
                    "line": 2,
                    "message": 3
                }
            ]
        }
    ]
}