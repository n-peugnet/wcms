{
    "problemMatcher": [
        {
            "owner": "phpunit",
            "fileLocation": "absolute",
            "pattern": [
                {
                    "regexp": "^\\d+\\)\\s.*$"
                },
                {
                    "regexp": "^(.*)$",
                    "message": 1
                },
                {
                    "regexp": "^\\s*$"
                },
                {
                    "regexp": "^(.*):(\\d+)$",
                    "file": 1,
                    "line": 2
                }
            ]
        }
    ]
}